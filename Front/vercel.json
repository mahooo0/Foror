{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*).js",
            "dest": "/public$1.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
